Tournament instant notes:

selectTournament()
	poolsCount = diversity * (selectionRatio * popSize)
	create poolsCount pools
        while _pop is not empty
       	     for each pool:
    	     	 push random individual from _pop
	     ;
        ;
	for each pool
	    rank pool by fitness
	;
	for each pool in pools
	    push best from pool into nextGen
	;
	i = 0
	while nextGen count < selectionRatio * popSize
	      push randomOrderedPick from pool[i] into nextGen
	      i = i + 1
	;
;
